<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".NapTienUser">
    <LinearLayout android:id="@+id/linearColumnarrowleft"
        android:layout_height="match_parent"
        android:layout_width="match_parent"
        android:orientation="vertical" style="@style/groupStylegray_600">
        <androidx.appcompat.widget.Toolbar android:gravity="start"
            android:id="@+id/toolbarToolbar"
            android:layout_height="wrap_content"
            android:layout_marginEnd="@dimen/_33pxh"
            android:layout_marginStart="@dimen/_33pxh"
            android:layout_width="match_parent" app:contentInsetStart="0dp">
            <LinearLayout android:id="@+id/linearRowarrowleft"
                android:layout_height="wrap_content"
                android:layout_width="match_parent" android:orientation="horizontal">
                <ImageView android:id="@+id/btnBack"
                    android:layout_height="@dimen/_14pxh"
                    android:layout_marginBottom="@dimen/_12pxv"
                    android:layout_marginTop="@dimen/_4pxv"
                    android:layout_width="@dimen/_14pxh"
                    android:scaleType="fitXY"
                    android:src="@drawable/img_arrowleft"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_arrowleft"/>
                <TextView android:ellipsize="end"
                    android:id="@+id/txtNaptin"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="@dimen/_103pxh"
                    android:layout_weight="0.88"
                    android:layout_width="0dp" android:singleLine="true"
                    style="@style/txtInterregular25" android:text="@string/lbl_na_p_ti_n"/>
            </LinearLayout>
        </androidx.appcompat.widget.Toolbar>
        <FrameLayout android:gravity="start"
            android:id="@+id/frameStackchonnhanhs"
            android:layout_height="match_parent"
            android:layout_marginTop="@dimen/_22pxv"
            android:layout_width="match_parent" style="@style/groupStylegray_600">

            <LinearLayout
                android:id="@+id/linearColumnellipsethree"
                style="@style/groupStylewhite_A702cornerRadius"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:orientation="vertical">

                <FrameLayout
                    android:id="@+id/frameStackellipsethree"
                    android:layout_width="match_parent"
                    android:layout_height="@dimen/_189pxv"
                    android:layout_marginTop="@dimen/_44pxv"
                    android:gravity="start">

                    <View
                        android:id="@+id/viewEllipseThree"
                        android:layout_width="match_parent"
                        android:layout_height="@dimen/_87pxv"
                        android:layout_gravity="top|start"
                        android:layout_marginBottom="@dimen/_10pxv"
                        android:background="@drawable/rectangle_bg_gray_600_radius_199_5" />

                    <LinearLayout
                        android:id="@+id/linearColumnsdkhadu"
                        style="@style/groupStylewhite_3"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_gravity="bottom|center"
                        android:layout_marginStart="@dimen/_22pxh"
                        android:layout_marginTop="@dimen/_10pxv"
                        android:layout_marginEnd="@dimen/_22pxh"
                        android:orientation="vertical">

                        <LinearLayout
                            android:id="@+id/linearRowsdkhadu"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_marginStart="@dimen/_10pxh"
                            android:layout_marginTop="@dimen/_25pxv"
                            android:layout_marginEnd="@dimen/_10pxh"
                            android:orientation="horizontal">

                            <TextView
                                android:id="@+id/txtSdkhadu"
                                style="@style/txtInterregular17"
                                android:layout_width="0dp"
                                android:layout_height="wrap_content"
                                android:layout_marginTop="@dimen/_1pxv"
                                android:layout_marginBottom="@dimen/_3pxv"
                                android:layout_weight="0.59"
                                android:ellipsize="end"
                                android:singleLine="true"
                                android:text="@string/msg_s_d_kha_du2" />

                            <TextView
                                android:id="@+id/txtSodu"
                                style="@style/txtIntersemibold21"
                                android:layout_width="0dp"
                                android:layout_height="wrap_content"
                                android:layout_marginStart="@dimen/_112pxh"
                                android:layout_weight="0.41"
                                android:ellipsize="end"
                                android:singleLine="true"
                                tools:text="@string/lbl_54_0002" />
                        </LinearLayout>

                        <EditText
                            android:id="@+id/txtTienNap"
                            style="@style/etSolidRoundedOutline_1"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_marginStart="@dimen/_10pxh"
                            android:layout_marginTop="@dimen/_16pxv"
                            android:layout_marginEnd="@dimen/_9pxh"
                            android:layout_marginBottom="@dimen/_28pxv"
                            android:background="@drawable/rectangle_bg_white_a701_border_bluegray_400_radius_15"
                            android:ellipsize="end"
                            android:hint="@string/msg_nh_p_s_ti_n"
                            android:inputType="number"
                            android:paddingStart="@dimen/_18pxh"
                            android:paddingTop="@dimen/_17pxv"
                            android:paddingEnd="@dimen/_30pxh"
                            android:paddingBottom="@dimen/_14pxv"
                            android:singleLine="true"
                            android:textColorHint="@color/gray_700"
                            tools:ignore="Autofill" />
                    </LinearLayout>
                </FrameLayout>

                <LinearLayout
                    android:id="@+id/linearColumnchonnhanhs"
                    style="@style/groupStylecornerRadius_1"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="@dimen/_23pxh"
                    android:layout_marginTop="@dimen/_25pxv"
                    android:layout_marginEnd="@dimen/_22pxh"
                    android:orientation="vertical">

                    <TextView
                        android:id="@+id/txtChonnhanhs"
                        style="@style/txtIntersemibold18_2"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_gravity="start"
                        android:layout_marginStart="@dimen/_8pxh"
                        android:layout_marginEnd="@dimen/_10pxh"
                        android:ellipsize="end"
                        android:singleLine="true"
                        android:text="@string/msg_cho_n_nhanh_s" />

                    <LinearLayout
                        android:id="@+id/linearRowtienone"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="@dimen/_14pxv"
                        android:orientation="horizontal">

                        <TextView
                            android:id="@+id/txtTienOne"
                            style="@style/txtSolidRounded"
                            android:layout_width="0dp"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="@dimen/_2pxv"
                            android:layout_weight="0.33"
                            android:ellipsize="end"
                            android:paddingStart="@dimen/_22pxh"
                            android:paddingTop="@dimen/_8pxv"
                            android:paddingBottom="@dimen/_10pxv"
                            android:singleLine="true"
                            android:text="@string/lbl_50_000"
                            android:tag="50000"

                            />

                        <TextView
                            android:id="@+id/txtTienTwo"
                            style="@style/txtSolidRounded"
                            android:layout_width="0dp"
                            android:layout_height="wrap_content"
                            android:layout_marginStart="@dimen/_19pxh"
                            android:layout_marginBottom="@dimen/_2pxv"
                            android:layout_weight="0.33"
                            android:ellipsize="end"
                            android:paddingStart="@dimen/_16pxh"
                            android:paddingTop="@dimen/_8pxv"
                            android:paddingBottom="@dimen/_10pxv"
                            android:singleLine="true"
                            android:text="@string/lbl_100_000"
                            android:tag="100000"
                            />

                        <TextView
                            android:id="@+id/txtTienThree"
                            style="@style/txtSolidRounded"
                            android:layout_width="0dp"
                            android:layout_height="wrap_content"
                            android:layout_marginStart="@dimen/_20pxh"
                            android:layout_marginBottom="@dimen/_2pxv"
                            android:layout_weight="0.33"
                            android:ellipsize="end"
                            android:paddingStart="@dimen/_16pxh"
                            android:paddingTop="@dimen/_8pxv"
                            android:paddingBottom="@dimen/_10pxv"
                            android:singleLine="true"
                            android:text="@string/lbl_200_000"
                            android:tag="200000"
                            />
                    </LinearLayout>

                    <LinearLayout
                        android:id="@+id/linearRowtienfive"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="@dimen/_15pxv"
                        android:orientation="horizontal">

                        <TextView
                            android:id="@+id/txtTienFour"
                            style="@style/etSolidRounded"
                            android:layout_width="0dp"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="@dimen/_2pxv"
                            android:layout_weight="0.33"
                            android:background="@drawable/rectangle_bg_gray_200_radius_7"
                            android:ellipsize="end"
                            android:inputType="text"
                            android:paddingStart="@dimen/_17pxh"
                            android:paddingTop="@dimen/_8pxv"
                            android:paddingEnd="@dimen/_13pxh"
                            android:paddingBottom="@dimen/_10pxv"
                            android:singleLine="true"
                            android:text="@string/lbl_500_000"
                            android:tag="500000"

                            android:textColorHint="@color/bluegray_900"
                            tools:ignore="Autofill" />

                        <TextView
                            android:id="@+id/txtTienFive"
                            style="@style/txtSolidRounded"
                            android:layout_width="0dp"
                            android:layout_height="wrap_content"
                            android:layout_marginStart="@dimen/_19pxh"
                            android:layout_marginBottom="@dimen/_2pxv"
                            android:layout_weight="0.33"
                            android:ellipsize="end"
                            android:paddingStart="@dimen/_10pxh"
                            android:paddingTop="@dimen/_8pxv"
                            android:paddingBottom="@dimen/_10pxv"
                            android:singleLine="true"
                            android:text="@string/lbl_1_000_000"
                            android:tag="1000000"
                            />

                        <TextView
                            android:id="@+id/txtTienSix"
                            style="@style/txtSolidRounded"
                            android:layout_width="0dp"
                            android:layout_height="wrap_content"
                            android:layout_marginStart="@dimen/_20pxh"
                            android:layout_marginBottom="@dimen/_2pxv"
                            android:layout_weight="0.33"
                            android:ellipsize="end"
                            android:paddingStart="@dimen/_9pxh"
                            android:paddingTop="@dimen/_8pxv"
                            android:paddingBottom="@dimen/_10pxv"
                            android:singleLine="true"
                            android:text="@string/lbl_2_000_000"
                            android:tag="2000000"
                            />
                    </LinearLayout>
                </LinearLayout>

                <androidx.appcompat.widget.AppCompatButton
                    android:id="@+id/btnNapTien"
                    style="@style/btnSolidRoundedOutline_1"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="@dimen/_23pxh"
                    android:layout_marginTop="@dimen/_159pxv"
                    android:layout_marginEnd="@dimen/_22pxh"
                    android:layout_marginBottom="@dimen/_20pxv"
                    android:ellipsize="end"
                    android:gravity="center"
                    android:paddingStart="@dimen/_30pxh"
                    android:paddingTop="@dimen/_15pxv"
                    android:paddingEnd="@dimen/_30pxh"
                    android:paddingBottom="@dimen/_15pxv"
                    android:singleLine="true"
                    android:text="@string/lbl_na_p_ti_n"
                    android:textAllCaps="false"
                    tools:text="@string/lbl_na_p_ti_n" />
            </LinearLayout>

            <LinearLayout android:id="@+id/linearSodukhadung"
                android:layout_gravity="top|start"
                android:layout_height="wrap_content"
                android:layout_marginBottom="@dimen/_10pxv"
                android:layout_width="match_parent"
                android:orientation="vertical" style="@style/groupStylegray_601">
                <TextView android:ellipsize="end"
                    android:id="@+id/txtMataikhoan"
                    android:layout_gravity="center_horizontal"
                    android:layout_height="wrap_content"
                    android:layout_marginEnd="@dimen/_118pxh"
                    android:layout_marginStart="@dimen/_126pxh"
                    android:layout_marginTop="@dimen/_20pxv"
                    android:layout_width="wrap_content"
                    android:singleLine="true"
                    style="@style/txtIntersemibold25_1" android:text="@string/lbl_ma_ta_i_khoa_n"/>
                <TextView
                    android:id="@+id/txtMaTK"
                    android:layout_gravity="center_horizontal"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="@dimen/_16pxv"
                    android:layout_marginEnd="@dimen/_50pxh"
                    android:layout_marginStart="@dimen/_50pxh"
                    android:layout_marginTop="@dimen/_6pxv"
                    android:layout_width="wrap_content"
                    android:singleLine="true"
                    style="@style/txtIntersemibold18_3" tools:text="@string/lbl_pd00001113"/>
            </LinearLayout>
        </FrameLayout>
    </LinearLayout>

</androidx.constraintlayout.widget.ConstraintLayout>